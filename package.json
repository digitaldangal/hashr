{
  "name": "@kobionic/hashr",
  "version": "1.0.0-SNAPSHOT",
  "private": false,
  "description": "An Electron/React powered file hashing application.",
  "keywords": [
    "Electron",
    "hash",
    "javascript",
    "node",
    "react",
    "typescript"
  ],
  "homepage": "https://gitlab.com/kobionic/hashr",
  "bugs": {
    "url": "https://gitlab.com/kobionic/hashr/issues"
  },
  "license": "Apache-2.0",
  "author": "Jeremie Rodriguez <contact@jeremierodriguez.com> (https://github.com/jeremiergz)",
  "main": "dist/main.js",
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/kobionic/hashr.git"
  },
  "scripts": {
    "build:dev": "concurrently 'npm run build:web-dev -- --progress false' 'npm run build:electron-dev'",
    "build:electron": "tsc --project tsconfig.electron.json",
    "build:electron-dev": "tsc --project tsconfig.electron.json --watch",
    "build:prod": "concurrently 'npm run build:web-prod' 'npm run build:electron'",
    "build:web": "webpack --config webpack.config.client.js --mode development --progress",
    "build:web-dev": "npm run build:web -- --watch",
    "build:web-prod": "npm run build:web -- --mode production --progress false",
    "start": "electron .",
    "start:dev": "ELECTRON_START_URL=http://localhost:3000 electron .",
    "start:web": "webpack-dev-server --config webpack.config.client.js --mode development --hot --inline"
  },
  "dependencies": {
    "@material-ui/core": "1.0.0-rc.0",
    "@material-ui/icons": "1.0.0-rc.0",
    "bytes": "3.0.0",
    "moment": "2.22.1",
    "ms": "2.1.1",
    "react": "16.3.2",
    "react-dom": "16.3.2"
  },
  "devDependencies": {
    "@types/bytes": "3.0.0",
    "@types/react": "16.3.14",
    "@types/react-dom": "16.0.5",
    "app-root-path": "2.0.1",
    "clean-webpack-plugin": "0.1.19",
    "concurrently": "3.5.1",
    "copy-webpack-plugin": "4.5.1",
    "css-loader": "0.28.11",
    "electron": "2.0.0",
    "electron-reload": "1.2.2",
    "file-loader": "1.1.11",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "3.2.0",
    "interpolate-html-plugin": "3.0.0",
    "mini-css-extract-plugin": "0.4.0",
    "optimize-css-assets-webpack-plugin": "4.0.1",
    "source-map-loader": "0.2.3",
    "ts-loader": "4.3.0",
    "tslint": "5.10.0",
    "tslint-react": "3.6.0",
    "typescript": "2.8.3",
    "typings-for-css-modules-loader": "1.7.0",
    "url-loader": "1.0.1",
    "webpack": "4.8.3",
    "webpack-cli": "2.1.3",
    "webpack-dev-server": "3.1.4",
    "webpack-merge": "4.1.2"
  },
  "publishConfig": {
    "access": "public"
  }
}
